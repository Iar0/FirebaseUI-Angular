{
  "name": "firebaseui-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build firebaseui-angular-library --prod && npx copy README.MD dist/firebaseui-angular-library",
    "pre-release": "cd projects/firebaseui-angular-library && npm version prerelease",
    "pre-patch": "cd projects/firebaseui-angular-library && npm version prepatch",
    "patch": "cd projects/firebaseui-angular-library && npm version patch",
    "pre-minor": "cd projects/firebaseui-angular-library && npm version preminor",
    "minor": "cd projects/firebaseui-angular-library && npm version minor",
    "pre-major": "cd projects/firebaseui-angular-library && npm version premajor",
    "major": "cd projects/firebaseui-angular-library && npm version major",
    "publish-dist": "npm run build-lib && npm publish dist/firebaseui-angular-library",
    "publish-dist-beta": "npm run build-lib && npm publish --tag beta dist/firebaseui-angular-library"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.2",
    "@angular/common": "6.0.2",
    "@angular/compiler": "6.0.2",
    "@angular/core": "6.0.2",
    "@angular/forms": "6.0.2",
    "@angular/http": "6.0.2",
    "@angular/platform-browser": "6.0.2",
    "@angular/platform-browser-dynamic": "6.0.2",
    "@angular/router": "6.0.2",
    "angularfire2": "^5.0.0-rc.10",
    "core-js": "^2.5.4",
    "firebase": "^5.0.4",
    "firebaseui": "^3.1.1",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular-devkit/build-ng-packagr": "~0.6.3",
    "@angular/cli": "6.0.3",
    "@angular/compiler-cli": "6.0.2",
    "@angular/language-service": "6.0.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "10.1.1",
    "codelyzer": "4.3.0",
    "copy": "^0.3.2",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "1.1.0",
    "ng-packagr": "^3.0.0-rc.4",
    "protractor": "^5.3.2",
    "ts-node": "6.0.3",
    "tsickle": "0.28.0",
    "tslib": "1.9.1",
    "tslint": "5.10.0",
    "typescript": "2.7.2"
  }
}
